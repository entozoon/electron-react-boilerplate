{
  "name": "electron-react-boilerplate",
  "version": "0.0.1",
  "description": "Store a list of media",
  "main": "./app/js/main.js",
  "scripts": {
    "refresh": "electron .",
    "dev": "npm-watch",
    "erm": "electron -r babel-register .",
    "dist": "electron-packager . --out=dist --overwrite --version-string.FileDescription=electron-app-template"
  },
  "repository": "https://github.com/entozoon/lists-of-valour",
  "keywords": [
    "lists",
    "imdb",
    "movies"
  ],
  "author": "MichaelCook <entozoon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.6.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-eslint": "^6.0.0",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^6.0.0",
    "electron-packager": "^8.6.0",
    "npm-watch": "^0.1.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "watch": {
    "refresh": {
      "patterns": "*",
      "extensions": "js,jsx,css",
      "quiet": true
    }
  }
}
